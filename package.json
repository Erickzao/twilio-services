{
  "name": "infra-test",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "test": "bun test",
    "migrate": "bun run src/database/migrations.ts",
    "typecheck": "node node_modules/typescript/bin/tsc --noEmit",
    "lint": "biome lint src",
    "lint:fix": "biome lint src --write",
    "format": "biome format src --write",
    "format:check": "biome format src",
    "check": "biome check src",
    "check:fix": "biome check src --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.9",
    "@types/bun": "^1.3.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/opentelemetry": "^1.4.9",
    "@elysiajs/swagger": "^1.3.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "cassandra-driver": "^4.8.0",
    "elysia": "^1.4.19",
    "twilio": "^5.10.7"
  }
}
